      {id:82,name:"Sticky Choc Chai",brand:"T2",type:"Chai",flavours:["Sweet", "Cocoa", "Spiced"],ingredients:"Black tea, honey, cinnamon, cloves, cardamom, ginger, cocoa husks, vanilla essence, salt",favourite:4,website:"",picture:"sticky-choc-chai.jpg"},
      {id:83,name:"Sticky Honey Chai",brand:"T2",type:"Chai",flavours:["Sweet", "Caramel", "Spiced"],ingredients:"Black tea, honey, cinnamon, cardamom, ginger, cloves, vanilla, salt",favourite:1,website:"https://www.t2tea.com/en/au/sticky-honey-chai-loose-leaf-refill-250g-T125AG071.html",picture:"sticky-honey-chai.jpg"},
      {id:84,name:"Sticky Honey Roobois",brand:"T2",type:"Rooibois",flavours:["Sweet", "Cinnamon", "Spiced"],ingredients:"Honey (35.8%), rooibos (17.9%), honeybush, cinnamon, cardamon pods, cardamon seed, cloves, peppercorns, ginger, salt",favourite:4,website:"https://www.t2tea.com/en/au/sticky-honey-rooibos-loose-leaf-refill-250g-T135AG019.html",picture:"sticky-honey-roobois.jpg"},
      {id:85,name:"Strawberry Rhubarb Parfait",brand:"David's Tea",type:"Fruit tea",flavours:["Fruity", "Creamy", "Berry"],ingredients:"Apple, Hibiscus, Raisins, Carrot, Artificial (strawberry, rhubarb) flavouring, Beetroot, Yogurt bits (skim milk yogurt powder, sugar, maltodextrin, modified starch, citric acid), Strawberry, Rhubarb.",favourite:2,website:"https://davidstea.com/products/strawberry-rhubarb-parfait-tea?variant=46532131619056",picture:"strawberry-rhubarb-parfait.webp"},
      {id:86,name:"Tummy Tea",brand:"T2",type:"Herbal tea",flavours:["Mint", "Floral", "Vegetal"],ingredients:"Peppermint, liquorice root, fennel, marigold petals CONTAINS LIQUORICE. People suffering from hypertension should avoid excessive consumption.",favourite:4,website:"https://www.t2tea.com/en/au/tummy-tea-tea-bag-refill-60-pack-B140AG054.html",picture:"tummy-tea.jpg"},
      {id:87,name:"WhiskTEA",brand:"Bird & Blend",type:"Rooibois",flavours:["Cinnamon", "Spiced", "Smoky"],ingredients:"Rooibos, Cinnamon, Ginger, Chinese Lapsang Souchong Black Tea, Safflower, Natural Flavouring",favourite:4,website:"https://www.birdandblendtea.us/products/whisktea?_pos=1&_psq=Whisktea&_ss=e&_v=1.0",picture:"whisk-tea.jpg"},
      {id:88,name:"Yorkshire Rose",brand:"Bird & Blend",type:"Black tea",flavours:["Floral", "Fruity", "Sweet"],ingredients:"Sri Lankan Black Tea, Apple Pieces, Hibiscus, Rosehip, Rose Petals, Freeze-dried Rhubarb, Flavouring",favourite:2,website:"https://www.birdandblendtea.us/products/yorkshire-rose-tea?_pos=1&_psq=yorkshire+rose&_ss=e&_v=1.0",picture:"yorkshire-rose.jpg"}
    ];

    // Extract unique types and flavours from data
    const teaTypes = [...new Set(teaData.map(t => t.type))].sort();
    const flavours = [...new Set(teaData.flatMap(t => t.flavours))].sort();

    // App state
    let currentScreen = 'home';
    let navigationStack = ['home'];
    let searchActive = false;
    let searchQuery = '';
    let selectedFilters = {types: [], flavours: []};
    let sortBy = 'alpha-asc';
    let selectedTea = null;
    let randomTea = null;
    let filterPanelOpen = false;
    let filterSubPanel = null;
    let randomFiltersOpen = false;
    let sortDropdownOpen = false;

    const sortOptions = [
      {value: 'alpha-asc', label: "Alphabetically, A-Z"},
      {value: 'alpha-desc', label: "Alphabetically, Z-A"},
      {value: 'favourites', label: "Renee's favourites"},
      {value: 'date-desc', label: "Date, new to old"},
      {value: 'date-asc', label: "Date, old to new"}
    ];

    // Icons
    const icons = {
      back: '<svg viewBox="0 0 24 24" fill="none" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M19 12H5M12 19l-7-7 7-7"/></svg>',
      search: '<svg viewBox="0 0 24 24" fill="none" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><circle cx="11" cy="11" r="8"/><path d="m21 21-4.35-4.35"/></svg>',
      chevron: '<svg viewBox="0 0 24 24" fill="none" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="m9 18 6-6-6-6"/></svg>',
      chevronDown: '<svg viewBox="0 0 24 24" fill="none" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="m6 9 6 6 6-6"/></svg>',
      close: '<svg viewBox="0 0 24 24" fill="none" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M18 6 6 18M6 6l12 12"/></svg>',
      filter: '<svg viewBox="0 0 24 24" fill="none" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><line x1="4" y1="8" x2="20" y2="8"/><line x1="6" y1="16" x2="18" y2="16"/></svg>',
      arrow: '<svg viewBox="0 0 24 24" fill="none" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M5 12h14M12 5l7 7-7 7"/></svg>',
      check: '<svg viewBox="0 0 24 24" fill="none" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"><polyline points="20 6 9 17 4 12"/></svg>'
    };

    // Render functions
    function render() {
      const app = document.getElementById('app');

      let html = '';

      switch (currentScreen) {
        case 'home':
          html = renderHeader('RENEE\'S TEA', false) + renderHomeMenu();
          break;
        case 'all-tea':
          html = renderHeader('ALL TEA', true) + renderFilterButton() + renderTeaList();
          break;
        case 'tea-by-type':
          html = renderHeader('TEA BY TYPE', true) + renderTypeList();
          break;
        case 'tea-by-type-detail':
          html = renderHeader(selectedFilters.types[0]?.toUpperCase() || 'TEA', true) + renderFilterButton() + renderTeaList();
          break;
        case 'tea-by-flavour':
          html = renderHeader('TEA BY FLAVOUR', true) + renderFlavourGrid();
          break;
        case 'tea-by-flavour-detail':
          html = renderHeader('TEA BY FLAVOUR', true) + renderFilterButton() + renderTeaList();
          break;
        case 'random-tea':
          html = renderHeader('RANDOM TEA', true) + renderRandomTea();
          break;
      }

      // Modal
      if (selectedTea) {
        html += renderTeaModal(selectedTea);
      }

      // Filter panel
      if (filterPanelOpen) {
        html += renderFilterPanel();
      }

      app.innerHTML = html;
      attachEventListeners();
    }

    function renderHeader(title, showBack) {
      return `
        <div class="header">
          ${showBack ? `<button class="header-back" onclick="goBack()">${icons.back}</button>` : ''}
          <span class="header-title">${title}</span>
          <div class="header-search-container">
            <input type="text" class="header-search-input ${searchActive ? 'expanded' : ''}" placeholder="Search..." value="${searchQuery}" oninput="updateSearch(this.value)" id="header-search-field">
            <button class="header-search-btn" onclick="toggleSearch()">${searchActive ? icons.close : icons.search}</button>
          </div>
        </div>
      `;
    }

    function renderHomeMenu() {
      if (searchQuery) {
        const teas = getFilteredTeas();
        return `
          <div class="menu-list">
            ${teas.length === 0
            ? '<div class="menu-item"><span class="menu-item-text">No teas found</span></div>'
            : teas.map(tea => `
                  <div class="menu-item" onclick="openTeaModal(${tea.id})">
                    <span class="menu-item-text">${tea.name}</span>
                    ${icons.chevron}
                  </div>
                `).join('')}
          </div>
        `;
      }
      return `
        <div class="menu-list">
          <div class="menu-item" onclick="navigateTo('all-tea')">
            <span class="menu-item-text">All Tea</span>
            ${icons.chevron}
          </div>
          <div class="menu-item" onclick="navigateTo('tea-by-type')">
            <span class="menu-item-text">Tea by Type</span>
            ${icons.chevron}
          </div>
          <div class="menu-item" onclick="navigateTo('tea-by-flavour')">
            <span class="menu-item-text">Tea by Flavour</span>
            ${icons.chevron}
          </div>
          <div class="menu-item" onclick="navigateTo('random-tea')">
            <span class="menu-item-text">Random Tea</span>
            ${icons.chevron}
          </div>
        </div>
      `;
    }

    function renderFilterButton() {
      return `
        <button class="filter-btn" onclick="openFilterPanel()">
          FILTER AND SORT
          ${icons.filter}
        </button>
      `;
    }

    function renderTeaList() {
      const teas = getFilteredTeas();
      return `
        <div class="tea-list">
          ${teas.map(tea => renderTeaCard(tea)).join('')}
          ${teas.length === 0 ? '<p style="text-align:center;padding:40px;color:#666;">No teas match your filters</p>' : ''}
        </div>
      `;
    }

    function renderTeaCard(tea) {
      return `
        <div class="tea-card" onclick="openTeaModal(${tea.id})">
          <div class="tea-card-name">${tea.name}</div>
          <div class="tea-card-type">${tea.type}</div>
          <div class="tea-card-brand">${tea.brand}</div>
          <div class="tea-card-tags">
            ${tea.flavours.slice(0, 3).map(f => `<span class="tea-tag">${f}</span>`).join('')}
          </div>
        </div>
      `;
    }

    function renderTeaModal(tea) {
      return `
        <div class="modal-overlay active" onclick="closeTeaModal(event)">
          <div class="modal-content" onclick="event.stopPropagation()">
            <button class="modal-close" onclick="closeTeaModal()">${icons.close}</button>
            <div class="modal-name">${tea.name}</div>
            <div class="modal-type">${tea.type}</div>
            <div class="modal-brand">${tea.brand}</div>
            <div class="modal-tags">
              ${tea.flavours.map(f => `<span class="tea-tag">${f}</span>`).join('')}
            </div>
            <div class="modal-section-title">Ingredients</div>
            <div class="modal-ingredients">${tea.ingredients}</div>
            ${tea.website ? `<a href="${tea.website.startsWith('http') ? '' : 'https://'}${tea.website}" target="_blank" class="modal-link">${tea.website.replace(/^https?:\/\//, '')}</a>` : ''}
            ${tea.picture ? `<img src="/renee-tea-catalogue/${tea.picture}" alt="${tea.name}" class="modal-image">` : ''}
          </div>
        </div>
      `;
    }

    function renderTypeList() {
      return `
        <div class="menu-list">
          ${teaTypes.map(type => `
            <div class="menu-item" onclick="selectType('${type}')">
              <span class="menu-item-text">${type}</span>
              ${icons.chevron}
            </div>
          `).join('')}
        </div>
      `;
    }

    function renderFlavourGrid() {
      return `
        <div class="flavour-grid">
          <div class="flavour-prompt">What do you feel like today?</div>
          ${flavours.map(f => `
            <button class="flavour-btn ${selectedFilters.flavours.includes(f) ? 'selected' : ''}" onclick="toggleFlavourSelect('${f}')">${f}</button>
          `).join('')}
        </div>
        <div class="apply-btn-container">
          <button class="apply-btn" onclick="applyFlavours()">
            APPLY
            ${icons.arrow}
          </button>
        </div>
      `;
    }

    function renderFilterPanel() {
      if (filterSubPanel === 'type') {
        return renderCheckboxPanel('Tea type', teaTypes, 'types');
      }
      if (filterSubPanel === 'flavour') {
        return renderCheckboxPanel('Flavour', flavours, 'flavours');
      }

      return `
        <div class="filter-panel active">
          <div class="header">
            <span class="header-title">ALL TEA</span>
          </div>
          <div class="filter-panel-header">
            <button class="filter-panel-close" onclick="closeFilterPanel()">${icons.close}</button>
          </div>
          <div class="filter-section">
            <div class="filter-section-title">FILTER BY</div>
          </div>
          <div class="filter-option-row" onclick="openFilterSubPanel('type')">
            <div class="filter-option-content">
              <span class="filter-option-text">Tea type</span>
              ${selectedFilters.types.length > 0 ? `<span class="filter-option-selected">${selectedFilters.types.join(', ')}</span>` : ''}
            </div>
            ${icons.chevron}
          </div>
          <div class="filter-option-row" onclick="openFilterSubPanel('flavour')">
            <div class="filter-option-content">
              <span class="filter-option-text">Flavour</span>
              ${selectedFilters.flavours.length > 0 ? `<span class="filter-option-selected">${selectedFilters.flavours.join(', ')}</span>` : ''}
            </div>
            ${icons.chevron}
          </div>
          <div class="filter-section">
            <div style="display:flex;align-items:center;justify-content:space-between;">
              <span class="filter-section-title" style="margin-bottom:0;">SORT BY</span>
              <div class="sort-select" onclick="toggleSortDropdown(event)">
                <span class="sort-value">${sortOptions.find(o => o.value === sortBy)?.label || 'Alphabetically, A-Z'}</span>
                ${icons.chevronDown}
                ${sortDropdownOpen ? `
                  <div class="sort-dropdown" onclick="event.stopPropagation()">
                    ${sortOptions.map(opt => `
                      <div class="sort-option" onclick="selectSort('${opt.value}')">
                        <span class="sort-option-check">${sortBy === opt.value ? icons.check : ''}</span>
                        <span>${opt.label}</span>
                      </div>
                    `).join('')}
                  </div>
                ` : ''}
              </div>
            </div>
          </div>
          <div class="filter-section" style="border-bottom:none;">
            <button class="reset-filters-btn" onclick="resetFilters()">Reset Filters</button>
          </div>
        </div>
      `;
    }

    function renderCheckboxPanel(title, items, filterKey) {
      return `
        <div class="filter-panel active">
          <div class="header">
            <button class="header-back" onclick="closeFilterSubPanel()">${icons.back}</button>
            <span class="header-title">ALL TEA</span>
            <button class="header-search">${icons.search}</button>
          </div>
          <div class="filter-panel-header">
            <button class="filter-panel-close" onclick="closeFilterPanel()">${icons.close}</button>
          </div>
          <div class="checkbox-list">
            ${items.map(item => `
              <div class="checkbox-item" onclick="toggleFilter('${filterKey}', '${item}')">
                <span class="checkbox-item-text">${item}</span>
                <div class="checkbox ${selectedFilters[filterKey].includes(item) ? 'checked' : ''}">
                  ${icons.check}
                </div>
              </div>
            `).join('')}
          </div>
        </div>
      `;
    }

    function renderRandomTea() {
      return `
        <button class="shuffle-btn" onclick="shuffleTea()">
          ${randomTea ? 'SHUFFLE AGAIN' : 'SHUFFLE'}
          ${icons.arrow}
        </button>
        <div class="filter-results-toggle ${randomFiltersOpen ? 'open' : ''}" onclick="toggleRandomFilters()">
          <span class="filter-results-text">Filter Results</span>
          ${icons.chevronDown}
        </div>
        ${randomFiltersOpen ? renderRandomFilters() : ''}
        ${randomTea ? `
          <div class="random-result">
            <div class="random-card">
              <button class="random-card-close" onclick="clearRandomTea()">${icons.close}</button>
              <div class="tea-card-name">${randomTea.name}</div>
              <div class="tea-card-type">${randomTea.type}</div>
              <div class="tea-card-brand">${randomTea.brand}</div>
              <div class="tea-card-tags">
                ${randomTea.flavours.slice(0, 3).map(f => `<span class="tea-tag">${f}</span>`).join('')}
              </div>
              <div class="modal-section-title" style="margin-top:20px;">Ingredients</div>
              <div class="modal-ingredients">${randomTea.ingredients}</div>
              ${randomTea.website ? `<a href="${randomTea.website.startsWith('http') ? '' : 'https://'}${randomTea.website}" target="_blank" class="modal-link">${randomTea.website.replace(/^https?:\/\//, '')}</a>` : ''}
              ${randomTea.picture ? `<img src="/renee-tea-catalogue/${randomTea.picture}" alt="${randomTea.name}" class="modal-image" style="margin-top:16px;">` : ''}
            </div>
          </div>
        ` : ''}
      `;
    }

    function renderRandomFilters() {
      return `
        <div class="menu-list" style="border-top:none;">
          <div class="menu-item" onclick="openFilterSubPanel('type'); filterPanelOpen = true; render();">
            <span class="menu-item-text">Tea type</span>
            ${icons.chevron}
          </div>
          <div class="menu-item" onclick="openFilterSubPanel('flavour'); filterPanelOpen = true; render();">
            <span class="menu-item-text">Flavour</span>
            ${icons.chevron}
          </div>
        </div>
      `;
    }

    // Helper functions
    function getFilteredTeas() {
      let filtered = [...teaData];

      if (searchQuery) {
        const q = searchQuery.toLowerCase();
        filtered = filtered.filter(t =>
          t.name.toLowerCase().includes(q) ||
          t.type.toLowerCase().includes(q) ||
          t.brand.toLowerCase().includes(q) ||
          t.flavours.some(f => f.toLowerCase().includes(q))
        );
      }

      if (selectedFilters.types.length > 0) {
        filtered = filtered.filter(t => selectedFilters.types.includes(t.type));
      }

      if (selectedFilters.flavours.length > 0) {
        filtered = filtered.filter(t =>
          t.flavours.some(f => selectedFilters.flavours.includes(f))
        );
      }

      // Sorting
      switch (sortBy) {
        case 'alpha-asc':
          filtered.sort((a, b) => a.name.localeCompare(b.name));
          break;
        case 'alpha-desc':
          filtered.sort((a, b) => b.name.localeCompare(a.name));
          break;
        case 'favourites':
          filtered.sort((a, b) => (a.favourite || 5) - (b.favourite || 5) || a.name.localeCompare(b.name));
          break;
        case 'date-desc':
          filtered.sort((a, b) => b.id - a.id);
          break;
        case 'date-asc':
          filtered.sort((a, b) => a.id - b.id);
          break;
      }

      return filtered;
    }

    // Navigation
    function navigateTo(screen) {
      navigationStack.push(screen);
      currentScreen = screen;
      if (screen === 'all-tea') {
        selectedFilters = {types: [], flavours: []};
      }
      render();
    }

    function goBack() {
      if (navigationStack.length > 1) {
        navigationStack.pop();
        currentScreen = navigationStack[navigationStack.length - 1];
        if (currentScreen === 'tea-by-type') {
          selectedFilters.types = [];
        }
        if (currentScreen === 'tea-by-flavour') {
          selectedFilters.flavours = [];
        }
        render();
      }
    }

    function toggleSearch() {
      searchActive = !searchActive;
      if (!searchActive) {
        searchQuery = '';
        clearTimeout(window.searchTimeout);
      }
      render();
      if (searchActive) {
        setTimeout(() => {
          const input = document.getElementById('header-search-field');
          if (input) input.focus();
        }, 350);
      }
    }

    function updateSearch(value) {
      searchQuery = value;
      // Debounce the render to avoid losing focus
      clearTimeout(window.searchTimeout);
      window.searchTimeout = setTimeout(() => {
        const input = document.getElementById('header-search-field');
        const cursorPos = input ? input.selectionStart : 0;
        renderContent();
        const newInput = document.getElementById('header-search-field');
        if (newInput) {
          newInput.focus();
          newInput.setSelectionRange(cursorPos, cursorPos);
        }
      }, 300);
    }

    function renderContent() {
      // Only re-render the content area, not the header
      const teaListEl = document.querySelector('.tea-list');
      const menuListEl = document.querySelector('.menu-list');

      if (teaListEl && (currentScreen === 'all-tea' || currentScreen === 'tea-by-type-detail' || currentScreen === 'tea-by-flavour-detail')) {
        const teas = getFilteredTeas();
        teaListEl.innerHTML = teas.length === 0
          ? '<p style="text-align:center;padding:40px;color:#666;">No teas match your search</p>'
          : teas.map(tea => renderTeaCard(tea)).join('');
      } else if (menuListEl && currentScreen === 'home') {
        const teas = getFilteredTeas();
        if (searchQuery) {
          menuListEl.innerHTML = teas.length === 0
            ? '<div class="menu-item"><span class="menu-item-text">No teas found</span></div>'
            : teas.map(tea => `
                <div class="menu-item" onclick="openTeaModal(${tea.id})">
                  <span class="menu-item-text">${tea.name}</span>
                  ${icons.chevron}
                </div>
              `).join('');
        }
      }
    }

    function selectType(type) {
      selectedFilters.types = [type];
      navigateTo('tea-by-type-detail');
    }

    function toggleFlavourSelect(flavour) {
      const idx = selectedFilters.flavours.indexOf(flavour);
      if (idx >= 0) {
        selectedFilters.flavours.splice(idx, 1);
      } else {
        selectedFilters.flavours.push(flavour);
      }
      render();
    }

    function applyFlavours() {
      if (selectedFilters.flavours.length > 0) {
        navigateTo('tea-by-flavour-detail');
      }
    }

    // Modal
    function openTeaModal(id) {
      selectedTea = teaData.find(t => t.id === id);
      render();
    }

    function closeTeaModal(event) {
      if (!event || event.target.classList.contains('modal-overlay')) {
        selectedTea = null;
        render();
      }
    }

    // Filter panel
    function openFilterPanel() {
      filterPanelOpen = true;
      filterSubPanel = null;
      render();
    }

    function closeFilterPanel() {
      filterPanelOpen = false;
      filterSubPanel = null;
      sortDropdownOpen = false;
      render();
      window.scrollTo(0, 0);
    }

    function openFilterSubPanel(panel) {
      filterSubPanel = panel;
      render();
    }

    function closeFilterSubPanel() {
      filterSubPanel = null;
      render();
    }

    function toggleFilter(key, value) {
      const idx = selectedFilters[key].indexOf(value);
      if (idx >= 0) {
        selectedFilters[key].splice(idx, 1);
      } else {
        selectedFilters[key].push(value);
      }
      // Save scroll position before render
      const checkboxList = document.querySelector('.checkbox-list');
      const scrollPos = checkboxList ? checkboxList.scrollTop : 0;
      render();
      // Restore scroll position after render
      const newCheckboxList = document.querySelector('.checkbox-list');
      if (newCheckboxList) {
        newCheckboxList.scrollTop = scrollPos;
      }
    }

    function toggleSortDropdown(event) {
      event.stopPropagation();
      sortDropdownOpen = !sortDropdownOpen;
      render();
    }

    function selectSort(value) {
      sortBy = value;
      sortDropdownOpen = false;
      render();
    }

    function resetFilters() {
      selectedFilters = {types: [], flavours: []};
      sortBy = 'alpha-asc';
      render();
    }

    // Random tea
    function shuffleTea() {
      const filtered = getFilteredTeas();
      if (filtered.length > 0) {
        randomTea = filtered[Math.floor(Math.random() * filtered.length)];
      }
      render();
    }

    function clearRandomTea() {
      randomTea = null;
      render();
    }

    function toggleRandomFilters() {
      randomFiltersOpen = !randomFiltersOpen;
      render();
    }

    function attachEventListeners() {
      // Handle back gesture / button
      window.onpopstate = function () {
        if (navigationStack.length > 1) {
          goBack();
        }
      };
    }

    // Initial render
    render();
  </script>
</body>

</html>
